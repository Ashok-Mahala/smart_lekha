{
  "name": "seatflow-sb2-admin-js",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "build:dev": "cd frontend && npm run build:dev",
    "build:prod": "cd frontend && npm run build:prod",
    "build:test": "cd frontend && npm run build:test",
    "lint": "cd frontend && npm run lint",
    "preview": "cd frontend && npm run preview",
    "backend:dev": "cd backend && npm run dev",
    "backend:build": "cd backend && npm run build",
    "backend:start": "cd backend && npm run start",
    "backend:prod": "cd backend && npm run start:prod",
    "backend:test": "cd backend && npm run test",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "start:dev": "concurrently \"npm run backend:dev\" \"npm run dev\"",
    "start:prod": "concurrently \"npm run backend:prod\" \"npm run preview\""
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "globals": "^16.0.0"
  },
  "dependencies": {
    "express-validator": "^7.2.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2"
  }
}
